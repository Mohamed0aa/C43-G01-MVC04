@model CreateEmployeeDto
@{
    ViewData["Title"] = "Delete";
}
<h1>Delete</h1>

<br />
<br />
<form asp-action="Delete">

    <input asp-for="ImageName" hidden/>

    <div class="mb-3">
        <label asp-for="Name" class="form-label">Name</label>
        <input type="text" class="form-control" asp-for="Name" aria-describedby="emailHelp">
        <span asp-validation-for="Name"></span>
    </div>
    <div class="mb-3">
        <label asp-for="Age" class="form-label">Age</label>
        <input type="text" class="form-control" asp-for="Age" aria-describedby="emailHelp">
        <span asp-validation-for="Age"></span>
    </div>
    <div class="mb-3">
        <label asp-for="Email" class="form-label">Email</label>
        <input type="email" class="form-control" asp-for="Email">
    </div>
    <div class="mb-3">
        <label asp-for="Phone" class="form-label">Phone</label>
        <input type="text" class="form-control" asp-for="Phone">
        <span asp-validation-for="Phone"></span>
    </div>
    <div class="mb-3">
        <label asp-for="Dept_id" class="form-label">Dept_id</label>
        <input type="text" class="form-control" asp-for="Dept_id">
        
    </div>
    <button type="submit" class="btn btn-danger">Delete</button>
</form>

